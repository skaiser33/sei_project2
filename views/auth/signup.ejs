<div class="logIn">
  <li><a class="landing-btn2" href="/auth/login">Login</a></li>
</div>
<div class="signup-container">
  <h1 class="signup-title">SignUp</h1>
  <form class="signUp" action="/auth/signup" method="POST">
    <input placeholder="Email"  class="login-email" id="new-email" type="email" name="email">
    <input placeholder="Username" class="login-email" id="new-name" type="text" name="name">
    <input placeholder="Password" class="login-email" id="new-password" type="password" name="password">
    <input class="login-submit-btn" type="submit" value="Submit">
  </form>
</div>
