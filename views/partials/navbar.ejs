<header>
  <nav>
    <ul>
      <div class="nav-container">
        <ul id="myLinks">
          <li class="home-link"><a href="./main">Home</a></li>
          <li><a href="./favorites">Favs</a></li>
          <li class="dropdown">
            <a href="javascript:void(0)" class="dropbtn">Comedians</a>
            <div class="dropdown-content">
              <form method="GET" action="/comedian" id="comedianform">
                <select name="comedian" id="comedian" form="comedianform" onchange="this.form.submit();">
                  <% allComedians.forEach((x)=> { %>
                  <option value=<%=x.id %>><%= x.name %></option>
                  <% }) %>
                </select>
              </form>
            </div>
          </li>
          <li class="dropdown">
            <a href="javascript:void(0)" class="dropbtn">Topics</a>
            <div class="dropdown-content">
              <form method="GET" action="/topic" id="topicform">
                <select name="topic" id="topic" form="topicform" onchange="this.form.submit();">
                  <% allTopics.forEach((idx)=> { %>
                  <option value=<%=idx.id %>><%= idx.name %></option>
                  <% }) %>
                </select>
              </form>
            </div>
          </li>
        <div class="end-nav">
          <li><a href="/profile">Account</a></li>
          <li><a href="/auth/logout">Logout</a></li>
        </div>
        </ul>
      </div>
    </ul>
  </nav>
</header>